{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "AI_KEY": "@AI_KEY",
        "EMAIL_USER": "@EMAIL_USER",
        "EMAIL_PASSWORD": "@EMAIL_PASSWORD"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "AI_KEY": "@AI_KEY",
        "EMAIL_USER": "@EMAIL_USER",
        "EMAIL_PASSWORD": "@EMAIL_PASSWORD"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
